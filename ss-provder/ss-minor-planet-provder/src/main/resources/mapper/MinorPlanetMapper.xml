<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rinkoai.mapper.MinorPlanetMapper">
    <select id="selectByNumber" resultType="com.rinkoai.ss.pojo.MinorPlanet">
        SELECT *
        FROM minor_planet
        WHERE number = #{number}
    </select>
</mapper>